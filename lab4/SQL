CREATE DATABASE Prescribe_DB;

CREATE TABLE PRESCRIPTION(
id INT,
drugname VARCHAR(50),
dosage VARCHAR(500),
PRIMARY KEY(id)
);

CREATE TABLE PATIENT_PRESCRIPTIONS(
patient_uname VARCHAR(20),
pres_id  INT,
FOREIGN KEY (pres_id) REFERENCES PRESCRIPTION (id),
PRIMARY KEY(patient_uname, pres_id)
);

CREATE DATABASE Report_DB;

CREATE TABLE REPORT(
text VARCHAR(500),
pres_id INT,
PRIMARY KEY(text, pres_id)
);
